<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape android:shape="rectangle">
        <gradient
        android:angle="180"
        android:startColor="#FFfa908a"
        android:endColor="#FFff0d00"
        android:type="linear" />
        <corners android:radius="@dimen/task_border"/>
    </shape>
</item>
<item>
    <shape android:shape="rectangle">
        <solid android:color="@color/taskTransparent"/>
        <corners android:radius="@dimen/task_border"/>
    </shape>
</item>
</layer-list>